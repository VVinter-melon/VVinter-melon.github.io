{
  "name": "jekyll-theme-skinny-bones",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint js/_main.js",
    "build:css": "npm run scss && npm run autoprefixer",
    "uglify": "uglifyjs js/*.js -m -o donkey/app.js && uglifyjs js/*.js -m -c -o donkey/app.min.js",
    "build:js": "npm run lint && npm run uglify"
  },
  "private": true,
  "description": "A Jekyll starter site",
  "author": "Michael Rose <michael@mademistakes.com>",
  "homepage": "http://mademistakes.com/",
  "license": "The MIT License (MIT)",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.15.1",
    "grunt": "~0.4.2",
    "grunt-contrib-imagemin": "~0.5.0",
    "grunt-contrib-jshint": "~0.8.0",
    "grunt-contrib-uglify": "~0.3.2",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-imgcompress": "~0.1.1",
    "grunt-newer": "^0.7.0",
    "grunt-surround": "^0.1.0",
    "grunt-svgmin": "~0.3.1",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.3",
    "uglify-js": "^3.6.0"
  }
}
